{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bbc-radio",
  "title": "BBC Radio Stations",
  "description": "Easily listen to BBC Radio stations from your Raycast",
  "icon": "command-icon.png",
  "author": "gabrythe23",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "BBC Radio",
      "description": "Template for showing a static list",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.3",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/node-cron": "^3.0.7",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "version": "1.0.1"
}
